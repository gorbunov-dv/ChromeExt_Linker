{
	"name": "Linker",
	"description": "Save important links for you.",
	"version": "1.0",

	"manifest_version": 2,
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "index.html",
		"default_title": "Save that link!"
	},
	"permission": [
		"activeTab",
		"http://*/",
		"storage"
	]
}